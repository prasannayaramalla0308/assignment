#include<stdio.h>
main()
{
int n,i=1,k,sum1=0,sum2=0;
printf("enter value of n\n");
scanf("%d",&n);
printf("enter value of k\n");
scanf("%d",&k);
while(i<=n)
{

	if(i%k==0)
	{
		printf("%d\n",i);
	if(i%2==0)
	{
		sum1=sum1+i;
	}
	else
	{
		sum2=sum2+i;
	}
 	}
    i++;
    
}
printf("even sum=%d\t",sum1);
printf("odd sum=%d",sum2);
}
